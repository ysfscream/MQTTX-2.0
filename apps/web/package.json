{
  "name": "mqttx-web",
  "version": "2.0.0-alpha.1",
  "description": "MQTTX CLI - An MQTT client for the command line",
  "author": "EMQX Team <yusf@emqx.io>",
  "homepage": "https://mqttx.app",
  "scripts": {
    "dev": "vite",
    "build:web": "run-p type-check \"build-only {@}\" --",
